{
  "name": "nupods/kernl-wp",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "Northeastern WP starter theme",
  "homepage": "https://assets.provost.northeastern.edu/kernl/wp/",
  "authors": [
    {
      "name": "NU PODS",
      "email": "provost-digital@northeastern.edu"
    }
